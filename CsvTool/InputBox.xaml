<Window x:Class="CsvTool.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CsvTool"
        mc:Ignorable="d"
        Title="InputBox" Height="172" Width="300">
    <Grid>
        <TextBox x:Name="InputTextBox" HorizontalAlignment="Left" Height="23" Margin="90,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="EnterBtn" Click="EnterBtn_Click" Content="确定" HorizontalAlignment="Left" Margin="60,84,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="CancelBtn" Click="CancelBtn_Click" Content="取消" HorizontalAlignment="Left" Margin="165,84,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="TipsTextBlock" TextAlignment="Center" HorizontalAlignment="Left" Margin="90,20,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
